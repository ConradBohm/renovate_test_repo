{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchUpdateTypes": ["major"]
    },
    {
      "matchPackageNames": ["numpy"],
      "dependencyDashboardApproval": true
    },
  ],
  "pip_requirements": {
    "fileMatch": [
      "requirements.yml"
    ]
  }
}
