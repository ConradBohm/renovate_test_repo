{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchUpdateTypes": ["major"],
      "asasignees": ["ConradBohm"]
    },
    {
      "matchPackageNames": ["numpy"],
      "labels": ["label for numpy packages"]
    },
  ],
  "pip_requirements": {
    "fileMatch": [
      "requirements.yml"
    ]
  }
}
